<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.manib.sunshinev2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.manib.sunshinev2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.manib.sunshinev2.DetailActivity"
            android:label="@string/title_activity_detail"
            android:parentActivityName="com.manib.sunshinev2.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.manib.sunshinev2.MainActivity" />
        </activity>
        <activity
            android:name="com.manib.sunshinev2.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.manib.sunshinev2.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.manib.sunshinev2.MainActivity" />
        </activity>
    </application>

</manifest>